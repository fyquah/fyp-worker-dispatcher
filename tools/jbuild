(executables
 ((names (now tree_tools data_cleaner local_reward_model feature_analysis))
  (preprocess (pps (ppx_jane  ppx_driver.runner)))
  (libraries
    (core
     async
     async_shell
  
     fyp_compiler_plugins
     fyp_compiler_lib
     protocol

     owl
     tensorflow
     tensorflow.core
     tensorflow.fnn
    ))
  ; (ocamlopt_flags
  ;   ((:standard -ccopt "-llapack -lopenblas")))
  ))

(install
 ((section bin)
  (files
    ((now.exe as ocamlnow)
     (tree_tools.exe as tree_tools)
     (data_cleaner.exe as data_cleaner)
     (feature_analysis.exe as feature_analysis)
     (local_reward_model.exe as local_reward_model)))))
