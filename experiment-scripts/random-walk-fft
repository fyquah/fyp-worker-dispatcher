#!/bin/bash

set -euo pipefail

cd "$(dirname "$0")"
cd ../

export EXPERIMENT_DIR="normal/fft"
export EXPERIMENT_BIN_NAME="fft"
export EXPERIMENT_BIN_ARGS="24"

if [ "$1" = "dev" ]; then
  scripts/run_dev.sh random-walk
elif [ "$1" = "prod" ]; then
  scripts/run_prod.sh random-walk
else
  echo "Unknown execution mode"
  exit 1
fi
